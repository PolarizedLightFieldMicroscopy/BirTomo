{
    "general": {
        "num_iterations": 300,
        "save_freq": 100,
        "output_directory_postfix": "VoxRingXZIt300Reg50Lr3",
        "notes": ""
    },
    "learning_rates": {
        "birefringence": 0.001,
        "optic_axis": 0.1
    },
    "file_paths": {
        "saved_rays": null
    },
    "regularization": {
        "weight": 1,
        "functions": [
            [
                "birefringence active L2",
                50
            ],
            [
                "birefringence active negative penalty",
                0
            ]
        ]
    },
    "schedulers": {
        "birefringence": {
            "type": "ReduceLROnPlateau",
            "params": {
                "mode": "min",
                "factor": 0.8,
                "patience": 5,
                "threshold": 1e-06,
                "min_lr": 1e-08
            }
        },
        "opticaxis": {
            "type": "CosineAnnealingWarmRestarts",
            "params": {
                "T_0": 100,
                "T_mult": 2,
                "eta_min": 0.0001
            }
        }
    },
    "nerf": {
        "enabled": false,
        "learning_rates": {
            "fc1": 0.001,
            "fc2": 1e-05,
            "fc3": 1e-05,
            "output": 1e-05
        },
        "optimizer": {
            "type": "NAdam",
            "betas": [
                0.9,
                0.999
            ],
            "eps": 1e-07,
            "weight_decay": 0.001
        },
        "scheduler": {
            "type": "CosineAnnealingLR",
            "params": {
                "T_max": 500,
                "eta_min": 1e-08
            }
        },
        "MLP": {
            "hidden_layers": [
                128,
                128,
                128
            ],
            "num_frequencies": 20,
            "final_layer_bias_birefringence": 0.05,
            "final_layer_weight_range": [
                -0.01,
                0.01
            ]
        }
    },
    "visualization": {
        "plot_live": true,
        "fig_size": [
            10,
            11
        ]
    },
    "learnables": {
        "all_prop_elements": false,
        "two_optic_axis_components": true
    },
    "misc": {
        "from_simulation": true,
        "save_ray_geometry": true,
        "optimizer": "Nadam",
        "datafidelity": "euler",
        "warmup_iterations": 10,
        "mla_rays_at_once": true,
        "free_memory_by_del_large_arrays": false
    }
}